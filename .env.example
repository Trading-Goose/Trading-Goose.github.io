# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
# Use the new publishable key format (recommended by Supabase, starts with sb_publishable_)
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here
# Legacy anon key (deprecated, only for backward compatibility)
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Development
APP_ENV=development

# Authentication
# Note: Invitations are now handled via Supabase Auth admin.inviteUserByEmail()
# Configure public registration settings in Supabase Dashboard > Auth > Settings
# Enable public registration (set to 'false' for invite-only mode)
ENABLE_PUBLIC_REGISTRATION=true